{
  "name": "manipulative-generator",
  "version": "1.0.0",
  "private": true,
  "description": "Edukaton project for manipulative content generation",
  "main": "start.js",
  "scripts": {
    "lint": "eslint './src/**/*{.js,.jsx}' './configs/**/*{.js,.jsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "concurrently -n \"generator\" -c \"blue\" \"yarn run build-generator\"",
    "analyze-generator": "ENTRY=generator babel-node configs/analyzeBundle.js",
    "build-generator": "ENTRY=generator babel-node configs/build.js",
    "start-generator": "babel-node configs/browserSync/generator.dev.js",
    "start-generator-prod": "babel-node configs/browserSync/generator.prod.js"
  },
  "author": "The Rothschilds",
  "license": "",
  "dependencies": {
    "async": "^2.6.0",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "moment-mini": "^2.19.4",
    "mousetrap": "^1.6.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.1.1",
    "react-dom-factories": "^1.0.2",
    "react-dropzone": "^4.2.3",
    "react-router-dom": "^4.2.2",
    "react-text-mask": "^5.1.0",
    "simple-react-form": "^2.0.1",
    "superagent": "^3.8.1",
    "typeface-roboto": "^0.0.45",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.13",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "css-reset": "^0.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "2.20.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "_moduleAliases": {
    "@src": "src",
    "@lib": "src/lib"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env",
          "react",
          "react-hmre",
          "stage-1"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-decorators-legacy",
          "transform-runtime"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "react",
          "stage-1"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-decorators-legacy",
          "transform-decorators-legacy",
          "transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "transform-runtime"
        ]
      }
    }
  }
}
